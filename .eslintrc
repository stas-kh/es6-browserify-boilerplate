{
	"extends": "airbnb-base",
	"rules": {
		"comma-dangle": 0,
		"consistent-return": 0,
		"default-case": 0,
		"class-methods-use-this": 0,
		"one-var": 0,
		"linebreak-style": [
			"error",
			"windows"
		],
		"indent": [2, "tab"],
		"no-tabs": 0
	},
	"env": {
		"browser": true
	},
	"globals": {
		"window": true,
		"document": true
	}
}
